{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { computed, reactive, ref } from \"vue\";\nimport dateHandler from \"@/composables/dateHandler\";\nimport Loading from \"@/components/Suspense/Loading\";\nimport axios from \"axios\";\nexport default {\n  __name: 'Week',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const {\n      getDay,\n      getDate,\n      isToday,\n      timeCalender,\n      dateCalender\n    } = dateHandler();\n    const apiData = ref();\n    const loading = ref(true);\n    const times = ['00:00', '02:00', '04:00', '06:00', '08:00', '10:00', '12:00', '14:00', '16:00', '18:00', '20:00', '22:00'];\n    const heightClass = ref('h-14');\n    axios.get('http://localhost:8000/api/calendar/week', {\n      headers: {\n        'Authorization': 'Bearer ' + localStorage.getItem('token')\n      }\n    }).then(response => response.data).then(response => {\n      apiData.value = response;\n      loading.value = false;\n    });\n    const weekDays = computed(() => {\n      const weekHolder = [];\n      if (apiData.value) {\n        apiData.value.week.forEach(value => {\n          weekHolder.push({\n            'day': getDay(value, 'name'),\n            'date': getDate(value, '/'),\n            'isToday': isToday(value)\n          });\n        });\n      }\n      return weekHolder;\n    });\n    const filterWeek = (date, time) => {\n      const moreThan = apiData.value.tasks.filter(x => dateCalender(x.task_date) === date.slice(0, 5) && timeCalender(x.task_time) === time);\n      if (moreThan.length > 2) {\n        return [{\n          id: 5,\n          title: '+' + moreThan.length + ' Opgaver',\n          moreTask: true\n        }];\n      } else {\n        return moreThan;\n      }\n    };\n    const openModel = id => {\n      console.log(id);\n    };\n    const __returned__ = {\n      getDay,\n      getDate,\n      isToday,\n      timeCalender,\n      dateCalender,\n      apiData,\n      loading,\n      times,\n      hei