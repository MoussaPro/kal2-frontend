{"ast":null,"code":"import { useRoute } from \"vue-router/dist/vue-router\";\nimport { onMounted, ref } from \"vue\";\nimport axios from \"axios\";\nimport { useRouter } from \"vue-router\";\nimport fieldHandler from \"@/composables/fieldHandler\";\nexport default {\n  __name: 'Field',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const {\n      addField,\n      deleteField\n    } = fieldHandler();\n    const route = useRoute();\n    const router = useRouter();\n    const apiData = ref();\n    const loading = ref(true);\n    const errorApiLoader = ref();\n    const error = ref();\n    const title = ref();\n    const errorMsg = ref();\n    const success = ref();\n    const successMsg = ref();\n    const testerData = ref(['æble', 'skorpion']);\n    const setFields = (obj, index) => {\n      apiData.value.fields[index] = obj;\n    };\n    onMounted(() => {\n      try {\n        axios.get('/field/' + route.params.id, {\n          headers: {\n            'Authorization': 'Bearer ' + localStorage.getItem('token')\n          }\n        }).then(response => response.data).then(response => {\n          apiData.value = response;\n          // Parse json\n          apiData.value.fields = JSON.parse(apiData.value.fields);\n          // For v-model to set checked, it need to be true instead of 1\n          if (apiData.value.is_primary === 1) {\n            apiData.value.is_primary = true;\n          }\n          title.value = apiData.value.title;\n          loading.value = false;\n        }).catch(response => {\n          errorApiLoader.value = response;\n        });\n      } catch (e) {\n        errorApiLoader.value = e;\n      }\n    });\n    const save = async () => {\n      error.value = false;\n      if (!title.value) {\n        error.value = true;\n        errorMsg.value = 'Angiv venligst et navn til dit skabelon';\n        return false;\n      }\n      apiData.value.fields.forEach(n => {\n        console.log(n.type);\n        if (!n.type) {\n          error.value = true;\n          errorMsg.value = 'Angiv venligst en type for felt';\n          return false;\n        }\n        if (!n.title) {\n          error.value = true;\n          errorMsg.value = 'Angiv venligst en overskrift for felt';\n          return false;\n        }\n      });\n      if (!loading.value && !error.value) {\n        loading.value = true;\n        await axios.put('/field/' + route.params.id, {\n          title: title.value,\n          fields: apiData.value.fields,\n          primary: apiData.value.is_primary\n        }, {\n          headers: {\n            'Authorization': 'Bearer ' + localStorage.getItem('token')\n          }\n        }).then(response => response.data).then(response => {\n          loading.value = false;\n          success.value = true;\n          successMsg.value = 'Opdateret';\n        }).catch(response => {\n          error.value = true;\n          errorMsg.value = 'Der opstod en fejl - fejlkode: ' + response.response.status;\n          loading.value = false;\n        });\n      } else {\n        return false;\n      }\n    };\n    const __returned__ = {\n      addField,\n      deleteField,\n      route,\n      router,\n      apiData,\n      loading,\n      errorApiLoader,\n      error,\n      title,\n      errorMsg,\n      success,\n      successMsg,\n      testerData,\n      setFields,\n      save,\n      get useRoute() {\n        return useRoute;\n      },\n      onMounted,\n      ref,\n      get axios() {\n        return axios;\n      },\n      get useRouter() {\n        return useRouter;\n      },\n      get fieldHandler() {\n        return fieldHandler;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"mappings":"AA4CE,SAASA,QAAQ,QAAQ,4BAA4B;AACrD,SAASC,SAAS,EAAEC,GAAG,QAAQ,KAAK;AACpC,OAAOC,KAAK,MAAM,OAAO;AACzB,SAASC,SAAS,QAAQ,YAAY;AACtC,OAAOC,YAAY,MAAM,4BAA4B;;;;;;;IACrD,MAAM;MAAEC,Q