{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { computed, reactive, ref } from \"vue\";\nimport dateHandler from \"@/composables/dateHandler\";\nimport timeHandler from \"@/composables/timeHandler\";\nimport Loading from \"@/components/Suspense/Loading\";\nimport axios from \"axios\";\nexport default {\n  __name: 'Week',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const {\n      times2Hours,\n      timeCalender,\n      timeBoxBoolean\n    } = timeHandler();\n    const {\n      getDay,\n      getDate,\n      isToday,\n      dateCalender\n    } = dateHandler();\n    const apiData = ref();\n    const loading = ref(true);\n    const heightClass = ref('h-14');\n    axios.get('http://localhost:8000/api/calendar/week', {\n      headers: {\n        'Authorization': 'Bearer ' + localStorage.getItem('token')\n      }\n    }).then(response => response.data).then(response => {\n      apiData.value = response;\n      loading.value = false;\n    });\n    const weekDays = computed(() => {\n      const weekHolder = [];\n      if (apiData.value) {\n        apiData.value.week.forEach(value => {\n          weekHolder.push({\n            'day': getDay(value, 'name'),\n            'date': getDate(value, '/'),\n            'isToday': isToday(value)\n          });\n        });\n      }\n      return weekHolder;\n    });\n    const filterWeek = (date, time) => {\n      const tasks = apiData.value.tasks.filter(task => {\n        const taskInBox = timeBoxBoolean(String(task.task_time), String(time + ':00'));\n        if (dateCalender(task.task_date) === date.slice(0, 5) && taskInBox) {\n          task.diff = taskInBox; // Assign the difference to the task object so we can use it in the component\n          return task;\n        }\n      });\n\n      // Return single task objects if no more than 3, otherwise return a \"more task\" object\n      return tasks.length < 3 ? tasks : [{\n        id: 5,\n        title: '+' + tasks.length + ' Opgaver',\n    