om \"@/composables/dateHandler\";\nimport { Auth } from \"@/store/auth\";\n\n  /** API TODO **/\n  /*\n  1. Ugedage skal være bold når det er idag\n  2. Week days\n  3. Opgaver\n  */\n\n/**\napiData.value.week.forEach((value) => {\n  weekDays.value.push({\n    'day': getDay(value, 'name'),\n    'date': getDate(value, '/'),\n    'isToday': isToday(value)\n  });\n});\n **/\n\n  const apiData = reactive({});\n\n  onMounted(() => {\n    fetch('http://localhost:8000/api/calendar/week')\n        .then((x) => x.json()).then((x) => {\n          apiData.value = x;\n    });\n\n    console.log(apiData)\n  })\n\n\n\n  const { getDay, getDate, isToday } = dateHandler();\n  const weekDays = ref([]);\n\n  const times = [\n    '00', '02', '04', '06', '08', '10', '12', '14', '16', '18', '20', '22'\n  ];\n\n  const heightClass = ref('h-14');\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}