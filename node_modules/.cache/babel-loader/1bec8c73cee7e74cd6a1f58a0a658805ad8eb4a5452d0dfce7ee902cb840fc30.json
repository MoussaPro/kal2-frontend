{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from 'vue';\nimport axios from 'axios';\nimport { useRouter } from 'vue-router';\nimport { Auth } from \"@/store/auth\";\nexport default {\n  __name: 'Login',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const auth = Auth();\n    const email = ref('');\n    const password = ref('');\n    const router = useRouter();\n    const submit = async () => {\n      const response = await axios.post('/login', {\n        email: email.value,\n        password: password.value\n      });\n      auth.login(response.data.access_token, response.data.user);\n      await router.push({\n        path: '/dashboard'\n      });\n    };\n    const __returned__ = {\n      auth,\n      email,\n      password,\n      router,\n      submit,\n      ref,\n      get axios() {\n        return axios;\n      },\n      get useRouter() {\n        return useRouter;\n      },\n      get Auth() {\n        return Auth;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"mappings":";AAmCE,SAASA,GAAG,QAAQ,KAAK;AACzB,OAAOC,KAAK,MAAM,OAAO;AACzB,SAASC,SAAS,QAAQ,YAAY;AACtC,SAASC,IAAI,QAAQ,cAAc;;;;;;;IACnC,MAAMC,IAAI,GAAGD,IAAI,EAAE;IAEnB,MAAME,KAAK,GAAGL,GAAG,CAAC,EAAE,CAAC;IACrB,MAAMM,QAAQ,GAAGN,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMO,MAAM,GAAGL,SAAS,EAAE;IAE1B,MAAMM,MAAM,GAAG,YAAY;MACzB,MAAMC,QAAQ,GAAG,MAAMR,KAAK,CAACS,IAAI,CAAC,QAAQ,EAAE;QAC1CL,KAAK,EAAEA,KAAK,CAACM,KAAK;QAClBL,QAAQ,EAAEA,QAAQ,CAACK;MACrB,CAAC,CAAC;MACFP,IAAI,CAACQ,KAAK,CAACH,QAAQ,CAACI,IAAI,CAACC,YAAY,EAAEL,QAAQ,CAACI,IAAI,CAACE,IAAI,CAAC;MAC1D,MAAMR,MAAM,CAACS,IAAI,CAAC;QAAEC,IAAI,EAAE;MAAa,CAAC,CAAC;IAC3C,CAAC","names":["ref","axios","useRouter","Auth","auth","email","password","router","submit","response","post","value","login","data","access_token","user","push","path"],"sources":["/Users/khaled.el-moussa/Desktop/Proj